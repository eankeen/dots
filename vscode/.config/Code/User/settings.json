{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.default.cStandard": "c11",
  "css.completion.triggerPropertyValueCompletion": false,
  "css.lint.duplicateProperties": "warning",
  "css.lint.emptyRules": "ignore",
  "css.lint.ieHack": "warning",
  "css.lint.vendorPrefix": "ignore",
  "css.lint.zeroUnits": "warning",
  "debug.node.autoAttach": "on",
  "diffEditor.maxComputationTime": 6000,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 3,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 3,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "'Fira Sans Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.minimap.maxColumn": 50,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.snippetSuggestions": "top",
  "editor.suggest.maxVisibleSuggestions": 10,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.wordWrapColumn": 100,
  "emmet.excludeLanguages": [
    "markdown",
    "c"
  ],
  "emmet.includeLanguages": {
    "vue-html": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.packageManager": "yarn",
  "explorer.autoReveal": false,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 4,
  "explorer.sortOrder": "mixed",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".vuerc": "json",
    "*.css": "postcss"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false
  },
  "files.simpleDialog.enable": true,
  "files.watcherExclude": {
    "**/bower_components/**": true
  },
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.defaultCloneDirectory": "$HOME/git",
  "gitlens.advanced.maxListItems": 300,
  "gitlens.advanced.maxSearchItems": 300,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "go.autocompleteUnimportedPackages": true,
  "html-css-class-completion.includeGlobPattern": "**/*.{css,html,vue}",
  "html.autoClosingTags": false,
  "html.format.enable": false,
  "html.format.wrapLineLength": 0,
  "javascript.referencesCodeLens.enabled": true,
  "less.lint.duplicateProperties": "warning",
  "less.lint.emptyRules": "ignore",
  "less.lint.ieHack": "warning",
  "less.lint.unknownVendorSpecificProperties": "warning",
  "less.lint.vendorPrefix": "ignore",
  "less.lint.zeroUnits": "warning",
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "material-icon-theme.files.associations": {
    ".babelignore": "babel",
    "tflint": "terraform"
  },
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showWelcomeMessage": false,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "merge-conflict.diffViewPosition": "Beside",
  "npm-intellisense.scanDevDependencies": true,
  "npm.bin": "yarn",
  "npm.packageManager": "yarn",
  "php.validate.run": "onType",
  "prettier.configPath": ".prettierrc",
  "prettier.endOfLine": "lf",
  "prettier.quoteProps": "consistent",
  "prettier.singleQuote": true,
  "python.analysis.diagnosticPublishDelay": 200,
  "python.analysis.logLevel": "Warning",
  "python.condaPath": "/opt/anaconda3",
  "python.dataScience.liveShareConnectionTimeout": 1500,
  "screencastMode.onlyKeyboardShortcuts": true,
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.emptyRules": "ignore",
  "scss.lint.ieHack": "warning",
  "scss.lint.unknownVendorSpecificProperties": "warning",
  "scss.lint.vendorPrefix": "ignore",
  "scss.lint.zeroUnits": "warning",
  "search-node-modules.useLastFolder": true,
  "search.exclude": {
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/yarn.lock": true
  },
  "search.location": "panel",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "sortJSON.contextMenu": {
    "sortJSON": false,
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONType": false,
    "sortJSONTypeReverse": false,
    "sortJSONValues": false,
    "sortJSONValuesReverse": false
  },
  "sortLines.filterBlankLines": true,
  "sortLines.sortEntireFile": true,
  "task.quickOpen.history": 20,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.shell.linux": "/bin/bash",
  "terminal.integrated.splitCwd": "workspaceRoot",
  "terraform.telemetry.enabled": false,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.tsserver.maxTsServerMemory": 1024,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.stylus": "none",
  "vetur.useWorkspaceDependencies": true,
  "vsdocker.imageUser": "docker.io/eankeen",
  "window.menuBarVisibility": "toggle",
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "all",
  "window.title": "ðŸ¦Š ${folderName} ${activeEditorLong}${separator}${remoteName}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": -1,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Nord",
  "workbench.commandPalette.history": 5,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.format.printWidth": 100,
  "yaml.format.singleQuote": true,
  "zenMode.centerLayout": true
}