export EDITOR=/usr/bin/vim
export QT_QPA_PLATFORMTHEME="qt5ct"
export QT_AUTO_SCREEN_SCALE_FACTOR=0
export GTK2_RC_FILES="$HOME/.gtkrc-2.0"

# hla or 'high assembly language'
export PATH=$PATH:/usr/local/bin/hla
export hlalib=/usr/local/bin/hla/hlalib
export hlainc=/usr/local/bin/hla/include

# xdg configs
export XDG_CONFIG_DIRS=/etc/xdg # default
export XDG_CONFIG_HOME=~/.config # default

